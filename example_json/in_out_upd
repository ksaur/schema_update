UPD ["order", ["orderitems"], "percentfullprice"]: {
$out = 1.0
# blatantly demonstrate usage of $\in, 
# (although could swap the next line with the prev)
$base['price'] = round($base["price"]/$in,2)
print $base["price"]
}

UPD ["order", "orderid"]: {
# demonstrate $\root
$out= $out + "_" + $root.get("orderdate").replace("/","")
}
